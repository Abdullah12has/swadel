<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <ImageButton
        android:id="@+id/btn_publicchathelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F8E7E3"
        app:srcCompat="@drawable/helpcircle"
        tools:layout_editor_absoluteX="337dp"
        tools:layout_editor_absoluteY="175dp" />

    <ImageButton
        android:id="@+id/btn_mentalhelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F8E7E3"
        app:srcCompat="@drawable/helpcircle"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="175dp" />

    <ImageButton
        android:id="@+id/btn_hwhelphelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F8E7E3"
        app:srcCompat="@drawable/helpcircle"
        tools:layout_editor_absoluteX="337dp"
        tools:layout_editor_absoluteY="16dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_browseResources"
        android:layout_width="155dp"
        android:layout_height="135dp"
        android:background="@drawable/rounded_corner_resource_btn"
        android:fontFamily="casual"
        android:text="Browse Resources"
        android:textColor="#FFF"
        android:textFontWeight="800"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.083"></androidx.appcompat.widget.AppCompatButton>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_MentalHealth"
        android:layout_width="155dp"
        android:layout_height="135dp"
        android:background="@drawable/rounded_corner_mentalhealth_btn"
        android:fontFamily="casual"
        android:text="Mental Health Services"
        android:textColor="#FFF"
        android:textFontWeight="800"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.427"></androidx.appcompat.widget.AppCompatButton>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_HWhelp"
        android:layout_width="155dp"
        android:layout_height="135dp"
        android:background="@drawable/rounded_corner_hw_btn"
        android:fontFamily="casual"
        android:text="Homework Help"
        android:textColor="#FFF"
        android:textFontWeight="800"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.804"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.083"></androidx.appcompat.widget.AppCompatButton>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_Chat"
        android:layout_width="155dp"
        android:layout_height="134dp"
        android:background="@drawable/rounded_corner_chat_btn"
        android:fontFamily="casual"
        android:text="Public Chat"
        android:textColor="#FFF"
        android:textFontWeight="800"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.804"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.424"></androidx.appcompat.widget.AppCompatButton>

    <ImageButton
        android:id="@+id/btn_resourcehelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F8E7E3"
        app:srcCompat="@drawable/helpcircle"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="16dp" />

</androidx.constraintlayout.widget.ConstraintLayout>